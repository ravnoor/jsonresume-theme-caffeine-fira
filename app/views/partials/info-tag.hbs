<div class="info-tag-container">
  {{#if fa-icon}}
    <i class="fa {{fa-icon}}"></i>
  {{/if}}

  {{#if ai-icon}}
    <i class="ai {{ai-icon}} ai-1x"></i>
  {{/if}}

  {{#if url}}
  <h6 class="info-text">
    <a href="{{url}}" target="_blank">
      {{removeProtocol text}} <i class="fa fa-external-link" aria-hidden="true"></i>
    </a>
  </h6>
  {{else}}
  <h6 class="info-text">{{{text}}}</h6>
  {{/if}}
</div>
